<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/fragment_thongke_manage"
    android:background="#fff"
    tools:context="vn.edu.poly.project_one.View_manage.ThongKe">
   <RelativeLayout
       android:layout_width="match_parent"
       android:layout_height="match_parent">
       <TextView
           android:id="@+id/txt_thongke_manage"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginLeft="20dp"
           android:layout_marginTop="25dp"
           android:text="@string/txt_thongke_manage"
           android:textColor="#000"
           android:textSize="34sp"
           android:textStyle="bold" />
       <ListView
           android:divider="@null"
           android:dividerHeight="20dp"
           android:id="@+id/lst_thongke"
           android:layout_below="@+id/txt_thongke_manage"
           android:layout_marginTop="30dp"
           android:layout_marginLeft="20dp"
           android:layout_marginRight="20dp"
           android:layout_width="match_parent"
           android:layout_height="match_parent"
           />

   </RelativeLayout>
</FrameLayout>
